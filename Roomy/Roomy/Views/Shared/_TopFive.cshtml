@model IEnumerable<Roomy.Models.Room>
@{
    Layout = null;
}

<div class="row">
    <div class="col-md-3">
        @foreach (var item in Model)
        {
            <div class="col-md-2">
                <p>@item.Name</p>
                <p>@item.Price €</p>
            </div>
        }
    </div>
</div>
